# A sample Gemfile
source "https://rubygems.org"

gem "jekyll"
gem "kramdown"
gem "redcarpet"

gem "jekyll-watch"
gem "pygments.rb"
gem "netrc"

group :jekyll_plugins do
    gem 'jekyll-github-metadata'
	gem 'github-pages', '109'
	gem "jekyll-admin"
	gem "jekyll-babel"
	gem "jekyll-paginate"
	gem "jekyll-bootstrap-sass"
    gem "jekyll-mentions"
	gem 'font-awesome-sass'
	gem 'jemoji'
end
