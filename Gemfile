# A sample Gemfile
source "https://rubygems.org"

gem "jekyll"
gem "kramdown", "1.10.0"
gem "octopress"

group :jekyll_plugins do
	gem "octopress-image-tag"
	gem "jekyll-babel"
	gem "jekyll-paginate"
	gem "octopress-autoprefixer"
	gem "jekyll-bootstrap-sass"
	gem 'font-awesome-sass'
	gem 'jemoji'
end
